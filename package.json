{
  "name": "comp30022-cra",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/auth": "^1.2.31",
    "aws-amplify": "^1.1.36",
    "aws-amplify-react": "^2.3.12",
    "logrocket": "^1.0.3",
    "logrocket-react": "^3.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "npm run build:css && react-scripts start",
    "build:css": "tailwind build ./src/styles/index.css -o ./src/styles/tailwind.css",
    "purge:css": "purgecss --css build/static/css/*.css --content build/index.html build/static/js/*.js --out build/static/css",
    "build": "npm run build:css && react-scripts build && npm run purge:css && npm run build-sw",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-sw": "node ./src/swconfig/sw-build.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "glob-all": "^3.1.0",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.18.2",
    "purgecss-webpack-plugin": "^1.5.0",
    "tailwindcss": "^1.1.2",
    "workbox-build": "^4.3.1"
  }
}
